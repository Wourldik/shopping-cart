{
  "name": "shopping-card",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint --format=stylish",
    "e2e": "ng e2e",
    "stylelint": "npm run stylelint:scss && npm run stylelint:css",
    "stylelint:scss": "stylelint --syntax=scss '**/*.scss'",
    "stylelint:css": "stylelint '**/*.{css,html}'",
    "tslint-check-conflict-rules": "tslint-config-prettier-check ./tslint.json",
    "lint-staged": "lint-staged -r -c lint-staged.linters.config.js",
    "reformat-staged": "lint-staged -r -c lint-staged.prettier.config.js",
    "mock:server": "json-server --watch ./src/app/features/mock-api/data/db.json",
    "start:mock:server": "concurrently --kill-others \"npm run mock:server\" \"npm run start\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.1",
    "@angular/cdk": "^10.0.0",
    "@angular/common": "~10.0.1",
    "@angular/compiler": "~10.0.1",
    "@angular/core": "~10.0.1",
    "@angular/forms": "~10.0.1",
    "@angular/material": "^10.0.0",
    "@angular/platform-browser": "~10.0.1",
    "@angular/platform-browser-dynamic": "~10.0.1",
    "@angular/router": "~10.0.1",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "lodash-es": "^4.17.15",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.2.0",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.0.5",
    "protractor": "~7.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-csstree-validator": "^1.8.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-high-performance-animation": "^1.5.1",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "stylelint-selector-tag-no-without-class": "^2.0.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-angular": "^3.0.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.9.5",
    "vrsource-tslint-rules": "^6.0.0"
  }
}
